{
  "name": "Rename Downloads",
  "description": "Change downloaded file names to other options",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["downloads", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "hello.html",
    "default_icon": "./images/icon.png"
  }
}
